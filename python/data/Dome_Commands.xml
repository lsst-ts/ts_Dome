<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsl"?>
<SALCommandSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://lsst-sal.tuc.noao.edu/schema/SALCommandSet.xsd">
<SALCommand>
    <EFDB_Topic>moveAz</EFDB_Topic>
    <Alias>moveAz</Alias>
    <item>
        <EFDB_Name>azimuth</EFDB_Name>
        <Description>Absolute positon angle for azimuth rotation in degrees</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>azRate</EFDB_Name>
        <Description>Specifies the signed velocity of crawl motion in degrees per second at which the motion should continue once the commanded azimuth has been reached. Positive means in the direction of increasing azimuth, negative in the direction of decreasing azimuth.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg/second</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>moveEl</EFDB_Topic>
    <Alias>moveEl</Alias>
    <item>
        <EFDB_Name>elevation</EFDB_Name>
        <Description>Absolute position angle for elevation motion in degrees</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>stopAz</EFDB_Topic>
    <Alias>stopAz</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>stopEl</EFDB_Topic>
    <Alias>stopEl</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>stop</EFDB_Topic>
    <Alias>stop</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>crawlAz</EFDB_Topic>
    <Alias>crawlAz</Alias>
    <item>
        <EFDB_Name>azRate</EFDB_Name>
        <Description>Specifies the signed velocity of crawl motion in degrees per second. Positive means in the direction of increasing azimuth, negative in the direction of decreasing azimuth.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg/second</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>crawlEl</EFDB_Topic>
    <Alias>crawlEl</Alias>
    <item>
        <EFDB_Name>elRate</EFDB_Name>
        <Description>Specifies the signed velocity of crawl motion in degrees per second. Positive means in the direction of increasing elevation, negative in the direction of decreasing elevation.</Description>
        <IDL_Type>double</IDL_Type>
        <Units>deg/second</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>setLouver</EFDB_Topic>
    <Alias>setLouver</Alias>
    <item>
        <EFDB_Name>id</EFDB_Name>
        <Description>Identification of louver to move</Description>
        <IDL_Type>int</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>position</EFDB_Name>
        <Description>The position to set the louver</Description>
        <IDL_Type>int</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>closeLouvers</EFDB_Topic>
    <Alias>closeLouvers</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>stopLouvers</EFDB_Topic>
    <Alias>stopLouvers</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>openShutter</EFDB_Topic>
    <Alias>openShutter</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>closeShutter</EFDB_Topic>
    <Alias>closeShutter</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>stopShutter</EFDB_Topic>
    <Alias>stopShutter</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>config</EFDB_Topic>
    <Alias>config</Alias>
    <item>
        <EFDB_Name>system</EFDB_Name>
        <Description>The lower level system for which to set the configuration value</Description>
        <IDL_Type>string</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>paramX</EFDB_Name>
        <Description>The configuration value for the specified parameter. There can be as many of these as necessary or wanted.</Description>
        <IDL_Type>string</IDL_Type>
        <Units>unitless</Units>
        <Count>n</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>park</EFDB_Topic>
    <Alias>park</Alias>
</SALCommand>
<SALCommand>
    <EFDB_Topic>setTemperature</EFDB_Topic>
    <Alias>setTemperature</Alias>
    <item>
        <EFDB_Name>temp</EFDB_Name>
        <Description>The temperature to set</Description>
        <IDL_Type>double</IDL_Type>
        <Units>Celsius</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>fans</EFDB_Topic>
    <Alias>fans</Alias>
    <item>
        <EFDB_Name>action</EFDB_Name>
        <Description>The action to execute: on or off</Description>
        <IDL_Type>string</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>inflate</EFDB_Topic>
    <Alias>inflate</Alias>
    <item>
        <EFDB_Name>action</EFDB_Name>
        <Description>The action to execute: on or off</Description>
        <IDL_Type>string</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
</SALCommand>
<SALCommand>
    <EFDB_Topic>status</EFDB_Topic>
    <Alias>status</Alias>
</SALCommand>
</SALCommandSet>
